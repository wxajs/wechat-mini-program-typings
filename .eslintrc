{
	"extends": ["eslint:recommended", "google"],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 9,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"rules": {
		"no-invalid-this": "off",
		"no-console": "off",
		"max-len": "off"
	},
	"env": {
		"node": true
	},
	"globals": {
		"console": true,
		"Promise": true
	}
}
